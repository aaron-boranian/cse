	znModel = CZM znTH = select( $Month >1 ,140,default 1)  znTD = select( $Month >1 ,145,default 2)  znTC = select( $Month >1 ,150,default 10)
        znQMxHRated = 1000000 znQMxCRated = -1000000      znQMxH = 1000000 znQMxC = -1000000
		 	 	
